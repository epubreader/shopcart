{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "never"],
        "no-console": "off",
        "quotes":["error", "single"],
        "no-underscore-dangle":"off",
        "import/extensions": "off",
        "no-unused-vars":"off"
    }
}
